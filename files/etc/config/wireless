
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/ahb/18100000.wmac'
	option band '2g'
	option country 'BO'
	option htmode 'HT20'
	option disabled '0'
	option legacy_rates '0'
	option cell_density '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'TollGate_c2d0cfda'
	option encryption 'none'
	option disabled '0'
	option vendor_elements 'dd0711223344556677'

config wifi-iface 'wifinet1'
	option device 'radio0'
	option mode 'sta'
	option network 'wwan'
	option ssid 'GL-MT300N-V2-d8d'
	option encryption 'psk-mixed'
	option key 'goodlife'

